{
    "name": "rekamy/timezone",
    "description": "Timezone List Plugin",
    "license": "MIT",
    "keywords": [
        "rekamy",
        "timezone",
        "timezones"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "laravel/framework": "^8.0 || ^7.0"
    },
    "authors": [
        {
            "name": "Firdaus Ishaddin",
            "email": "firdausishaddin@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rekamy\\Generator\\GeneratorServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rekamy\\Timezone\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../rekamy/timezone"
        }
    ]
}
